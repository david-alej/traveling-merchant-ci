  "scripts": {
    "lint": "eslint ./",
    "lint-fix": "eslint ./ --fix",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npm run seed",
