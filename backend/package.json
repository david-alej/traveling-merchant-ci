    "test": "cross-env NODE_ENV=test mocha ./test/main.js --testTimeout=10000 --exit",
    "raw-test": "cross-env NODE_ENV=test mocha ./test/main.js --testTimeout=10000 --exit",
    "mutate": "npm run pretest && cross-env NODE_ENV=test npx stryker run",
    "debug-docker": "nodemon --inspect=0.0.0.0:9229 ./src/index.js",
    "dev": "cross-env CI_BACKEND= nodemon src/index.js",
    "start": "node src/index.js"
